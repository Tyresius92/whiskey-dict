{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.0.0",
    "@fortawesome/free-regular-svg-icons": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@radix-ui/react-separator": "^0.1.4",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/styled-system": "^5.1.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "styled-components": "^5.3.3",
    "styled-system": "^5.1.5",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --ignore-path .prettierignore --parser typescript --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --ignore-path .prettierignore --parser typescript --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "node_modules/.bin/eslint --color --ext js,jsx,ts,tsx ./src",
    "lint:fix": "node_modules/.bin/eslint --color --fix --ext js,jsx,ts,tsx ./src",
    "fix": "npm run prettier:fix && npm run lint:fix && npm run sniff",
    "smellsGood": "node scripts/echo \"Great success!! Your code smells goooood!\" --font-color 92",
    "sniff": "npm run prettier && npm run lint && npm run smellsGood",
    "storybook": "DISABLE_ESLINT_PLUGIN=true start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/node-logger": "^6.4.19",
    "@storybook/preset-create-react-app": "^4.0.1",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "0.0.9",
    "@types/styled-components": "^5.1.24",
    "eslint-plugin-testing-library": "^5.0.6",
    "prettier": "2.5.1",
    "webpack": "^5.70.0"
  }
}
